{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "REACT_APP_NODE_ENV": "production",
    "REACT_APP_API_URL": "https://backend.lasertrader.co/api"
  }
}
